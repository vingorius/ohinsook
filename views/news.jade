block variables
	- title = "Photographer Oh,Insook News";
	- description = "Photographer Oh,Insook News";

extends modules/baselayout

block content
	div#photoArtModal.modal.fade(tabindex='-1', role='dialog', aria-labelledby='myModalLabel', aria-hidden='true')
		div.modal-dialog(style={'width':'1200px'})
			div.modal-content
				div.modal-body
					img.img-responsive(src='images/news/photoart_orig.jpg')

	div#photoPlusModal.modal.fade(tabindex='-1', role='dialog', aria-labelledby='myModalLabel', aria-hidden='true')
		div.modal-dialog.modal-lg
			div.modal-content
				div.modal-body
					div#carousel-photoart.carousel.slide(data-ride='carousel')
						// Indicators
						ol.carousel-indicators
							li.active(data-target='#carousel-photoart', data-slide-to='0')
							li(data-target='#carousel-photoart', data-slide-to='1')
							li(data-target='#carousel-photoart', data-slide-to='2')
						// Wrapper for slides
						div.carousel-inner(role='listbox')
							div.item.active
								img(src='images/news/photoplus/1.jpg', alt='...')
								div.carousel-caption
									| ...
							div.item
								img(src='images/news/photoplus/2.jpg', alt='...')
								div.carousel-caption
									| ...
							div.item
								img(src='images/news/photoplus/3.jpg', alt='...')
								div.carousel-caption
									| ...
						a.left.carousel-control(href='#carousel-photoart', role='button', data-slide='prev')
							span.glyphicon.glyphicon-chevron-left(aria-hidden='true')
							span.sr-only Previous
						a.right.carousel-control(href='#carousel-photoart', role='button', data-slide='next')
							span.glyphicon.glyphicon-chevron-right(aria-hidden='true')
							span.sr-only Next


	div#main.container.main-margin
