					td
						blockquote= __('news.photoart.quote')
							small= __('news.photoart.press')
					td
						a(href='#')
							img.hvr-grow.work_image(alt="#{__('news.photoart.press')}", src='images/news/photoart.jpg', data-toggle="modal", data-target="#photoArtModal")

				tr
					td
						blockquote= __('news.thisman.quote')
